import java.util.*;
public class BasicCalculator {

	 public static void main(String[] args) {
		 Scanner sc=new Scanner(System.in);
		 int n=sc.nextInt();
		 int m=sc.nextInt();
		 char operator=sc.next().charAt(0);
		 switch(operator) {
		 case '+':
			 System.out.println(n+m);
			 break;
		 case '-':
			 System.out.println(n-m);
			 break;
		 case '*':
			 System.out.println(n*m);
			 break;
		 case '/':
			 System.out.println(n/m);
			 break;
		 case '%':
			 System.out.println(n%m);
			 break;
		default:
			System.out.println("Invalid input");
			
		 }
	 }

}
